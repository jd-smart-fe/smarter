#!/usr/bin/env node

var program = require('commander');

program
  .version(require('../package.json').version)
  // .command('init', 'Init a smarter workplace; 初始化一个 smater 工作区')
  .command('create [template]', 'Generate a project template; 生成一个项目模板')
  .parse(process.argv);


process.on('exit', function () {
  console.log();
});

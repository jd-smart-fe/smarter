#!/usr/bin/env node

const program = require('commander');

program
  .version(require('../package.json').version)
  // .command('init', 'Init a smarter workplace; 初始化一个 smater 工作区')
  .command('create [template]', '创建一个项目脚手架')
  // .command('env [settings]', '生成各种工作区环境配置文件')
  // .command('upload', '上传文件到服务器')
  .parse(process.argv);

process.on('exit', () => {
  console.log();
});
